<header>
  <%= link_to image_tag("/assets/headerimage-43044260c9c00ae390f6603ecb4c3801714aa8ef7793b4902d67d40e86565875.png"), "/" , {class: "headerimage"}%>
  <%= link_to "カメラマン新規登録", new_photographer_path %>
  <%= link_to "ログイン", new_session_path%>
      <nav>
      <ul class="nav navbar-nav navbar-right">
        <% if logged_in? %>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              設定 <b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to "マイページ", current_photographer %></li>
              <li><%= link_to 'プロフィール編集',edit_photographer_path(current_photographer) %></li>
              <li class="divider"></li>
              <li><%= link_to "ログアウト", session_path(current_photographer) , method: :delete %></li>
            </ul>
          </li>
        <% else %>
          <span class="glyphicon glyphicon-user" aria-hidden="true"></span>
          <li><%= link_to "ログイン", new_session_path %></li>
        <% end %>
      </ul>
    </nav>
</header>